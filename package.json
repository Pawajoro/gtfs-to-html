{
  "name": "gtfs-to-html",
  "description": "Build human readable transit timetables in HTML and PDF from GTFS",
  "version": "0.20.0",
  "keywords": [
    "transit",
    "gtfs",
    "transportation",
    "timetables"
  ],
  "private": false,
  "author": "Brendan Nee <brendan@blinktag.com>",
  "contributors": [
    "Evan Siroky",
    "Nathan Selikoff",
    "Aaron Antrim <aaron@trilliumtransit.com>",
    "thomastrillium",
    "hkvlhm",
    "Pawajoro"
  ],
  "repository": "git://github.com/blinktaginc/gtfs-to-html",
  "bugs": {
    "url": "https://github.com/blinktaginc/gtfs-to-html/issues"
  },
  "license": "MIT",
  "scripts": {
    "start": "node ./app",
    "test": "xo"
  },
  "bin": {
    "gtfs-to-html": "bin/gtfs-to-html.js"
  },
  "main": "index",
  "dependencies": {
    "@turf/simplify": "^5.1.5",
    "archiver": "^4.0.1",
    "chalk": "^4.1.0",
    "cli-table": "^0.3.1",
    "express": "^4.17.1",
    "fs-extra": "^9.0.1",
    "gtfs": "1.10.2",
    "js-beautify": "^1.11.0",
    "lodash": "^4.17.15",
    "moment": "^2.27.0",
    "mongoose": "^5.9.19",
    "morgan": "^1.10.0",
    "pretty-error": "^2.1.1",
    "progress": "^2.0.3",
    "pug": "^3.0.0",
    "puppeteer": "^4.0.0",
    "sanitize-filename": "^1.6.3",
    "timer-machine": "^1.1.0",
    "untildify": "^4.0.0",
    "yargs": "^15.3.1"
  },
  "devDependencies": {
    "xo": "^0.32.0"
  },
  "engines": {
    "node": ">= 12.14.0"
  },
  "xo": {
    "esnext": true,
    "space": true,
    "rules": {
      "camelcase": [
        "error",
        {
          "properties": "never"
        }
      ],
      "object-curly-spacing": [
        "error",
        "always"
      ]
    }
  },
  "false": {}
}
